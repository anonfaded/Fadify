{
  "manifest_version": 2,
  "name": "Fadify",
  "version": "1.0",
  "description": "Theme labs for ChatGPT and more sites to come",
  "permissions": ["tabs", "activeTab", "storage"],
  "content_security_policy": "default-src 'self'; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com https://cdnjs.cloudflare.com; font-src 'self' https://fonts.gstatic.com https://cdnjs.cloudflare.com; script-src 'self'",
  "icons": {
    "16": "assets/fadseclab/fadify.png",
    "32": "assets/fadseclab/fadify.png",
    "48": "assets/fadseclab/fadify.png",
    "128": "assets/fadseclab/fadify.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "assets/fadseclab/fadify.png",
      "32": "assets/fadseclab/fadify.png",
      "48": "assets/fadseclab/fadify.png",
      "128": "assets/fadseclab/fadify.png"
    },
    "default_popup": "popup/popup.html",
    "default_title": "Fadify"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["*://chat.openai.com/*", "*://chatgpt.com/*"],
  "js": ["utils/preferences.js", "labs/chatgpt/ChatGPTContainer.js", "labs/chatgpt/chatgpt.js"],
      "css": ["labs/chatgpt/chatgpt.css"]
    }
  ],
  "web_accessible_resources": [
    "assets/space-background-webm.webm",
    "data/apps.json"
  ]
}
